{
  "name": "feathers-docs-mcp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Read-only MCP server for FeathersJS documentation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test:e2e": "npm run build && vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "gray-matter": "^4.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@playwright/test": "^1.54.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^2.1.8"
  }
}